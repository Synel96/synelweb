{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "synelweb.hu" }
      ],
      "destination": "https://www.synelweb.hu/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
